@model rhBugTracker.Models.Dashboard
@{
    ViewBag.Title = "Home Page";
}

<!--SUBMITTER DASHBOARD-->
    
<!---------------MAIN CONTENT ----------------------->
<div class="container" style="padding: 10px 10px 10px 10px">    
    <div class="main-content-inner">
        <div class="row">
            <!-- seo fact area start -->
            <div class="col-lg-12">
                <div class="row">
                    <div class="col-md-4 ">
                        <div class="card">
                            <div class="seo-fact sbg1">
                                <div class="p-4 d-flex justify-content-between align-items-center">
                                    <a href="@Url.Action("myTickets", "Tickets")"><div class="seofct-icon"><i class="ti-ticket"></i>My Tickets</div></a>
                                    <h2></h2>
                                </div>
                                @*<canvas id="seolinechart1" height="50"></canvas>*@
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4 ">
                        <div class="card">
                            <div class="seo-fact sbg1">
                                <div class="p-4 d-flex justify-content-between align-items-center">
                                    <a href="@Url.Action("myProjects", "Projects")"><div class="seofct-icon"><i class="ti-blackboard"></i>My Projects</div></a>
                                    <h2></h2>
                                </div>
                                @*<canvas id="seolinechart1" height="50"></canvas>*@
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4 ">
                        <div class="card">
                            <div class="seo-fact sbg1">
                                <div class="p-4 d-flex justify-content-between align-items-center">
                                    <a href="@Url.Action("Create", "Tickets")"><div class="seofct-icon"><i class="ti-file"></i>Create Ticket</div></a>
                                </div>
                                @*<canvas id="seolinechart1" height="50"></canvas>*@
                            </div>
                        </div>
                    </div>

                </div>
            </div>
            <!-- seo fact area end -->
        </div>
    </div>
</div>
